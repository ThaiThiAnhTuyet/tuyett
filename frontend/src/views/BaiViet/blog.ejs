    <!-- Plugins CSS (All Plugins Files) -->
    <link rel="stylesheet" href="/static/css/plugins/swiper-bundle.min.css">
    <link rel="stylesheet" href="/static/css/plugins/font-awesome.min.css">
    <link rel="stylesheet" href="/static/css/plugins/simple-line-icons.css">
    <link rel="stylesheet" href="/static/css/plugins/fancybox.min.css">
    <link rel="stylesheet" href="/static/css/plugins/nice-select.css">
    <link rel="stylesheet" href="/static/css/plugins/range-slider.css">

    <!-- Style CSS -->
    <link rel="stylesheet" href="/static/css/style.css">

</head>

<body>

    <!--== Wrapper Start ==-->
    <div class="wrapper">


        <!--== Start Header Wrapper ==-->
        <header class="header-wrapper">
            <div class="header-top d-none d-md-block">
                <div class="container">
                    <div class="header-top-area">
                        <div class="header-top-info">
                            <a href="/home">Hoàn toàn miễn phí trả lại và miễn phí vận chuyển trên toàn thế giới</a>
                        </div>
                        <div class="header-top-action-area">
                            <div class="header-info-dropdown">
                                <button type="button" class="btn-info dropdown-toggle" id="dropdownLang"
                                    data-bs-toggle="dropdown" aria-expanded="false">VIE</button>
                                <ul class="dropdown-menu" aria-labelledby="dropdownLang">
                                    <li class="dropdown-item active">VIE</li>
                                    <li class="dropdown-item">ENG</li>
                                </ul>
                            </div>
                            <div class="header-info-dropdown">
                                <button type="button" class="btn-info dropdown-toggle" id="dropdownCurrency"
                                    data-bs-toggle="dropdown" aria-expanded="false">VND</button>
                                <ul class="dropdown-menu" aria-labelledby="dropdownCurrency">
                                    <li class="dropdown-item active">USD</li>
                                    <li class="dropdown-item">VND</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="header-middle">
                <div class="container">
                    <div class="row align-items-center justify-content-between align-items-center">
                        <div class="col-auto">
                            <div class="header-logo-area">
                                <a href="/home">
                                    <img class="logo-main" src="/static/images/logo.png" width="182" height="31"
                                        alt="Logo">
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="header-area d-none d-lg-block">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-auto d-none d-lg-block ms-auto me-n3 pe-0">
                            <div class="header-navigation ps-6">
                                <%- include('../partical/menu') %>
                            </div>
                        </div>
                        <div class="col-auto ">
                            <% if (user) { %>
                                <div class="header-action-user d-flex align-items-center gap-2">
                                    <a href="/account" class="d-flex align-items-center">
                                        <i class="icon icon-user me-1"></i>
                                        <span style="color: rgb(60, 252, 12);">
                                            <%= user.name %>
                                        </span>
                                    </a>
                                    <a href="/logout" title="Đăng xuất" class="ms-2"
                                        style="font-size: 20px; color: rgb(245, 12, 12);">
                                        <i class="fa fa-power-off"></i>
                                    </a>
                                </div>
                                <% } else { %>
                                    <a href="/login" class="header-action-user d-flex align-items-center">
                                        <i class="icon icon-user me-1"></i> Đăng nhập
                                    </a>
                                    <% } %>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!--== End Header Wrapper ==-->


        <main class="main-content">

            <!--== Start Page Header Area Wrapper ==-->
            <div class="page-header-area">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="page-header-content">
                                <h2 class="page-header-title">Bài Viết </h2>
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="/home">Trang Chủ //</a></li>
                                    <li class="breadcrumb-item active" aria-current="page">Bài Viết</li>
                                </ol>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="page-header-thumb">
                                <img src="/static/images/photos/page-title.png" alt="Image-HasTech" width="546"
                                    height="281">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--== End Page Header Area Wrapper ==-->

            <!--== Start Blog Grid Area Wrapper ==-->
            <div class="blog-grid-area section-space">
                <div class="container">
                    <div class="row mb-n6 post-border-items">
                        <% blogs.forEach(blog=> { %>
                            <div class="col-sm-6 col-md-6 col-lg-4 mb-6">
                                <!--== Start Blog Item ==-->
                                <div class="post-item">
                                    <a href="/blogdetails?id=<%= blog.id %>" class="post-item-thumb">
                                        <img class="w-100" src="<%= blog.thumbnail %>" width="370" height="244"
                                            alt="<%= blog.title %>">
                                    </a>
                                    <div class="post-item-content">
                                        <div class="post-item-meta">
                                            <div class="post-item-date">
                                                <i class="icon icon-calendar"></i>
                                                <%= blog.createdAt.toLocaleDateString("vi-VN") %>
                                            </div>
                                            <div class="post-item-author">
                                                <i class="icon icon-user"></i>
                                                <%= blog.author %>
                                            </div>
                                        </div>
                                        <h4 class="post-item-title mb-2">
                                            <a href="/blogdetails?id=<%= blog.id %>">
                                                <%= blog.title %>
                                            </a>
                                        </h4>
                                        <p class="post-item-desc">
                                            <%= blog.content.substring(0, 100) %>...
                                        </p>
                                        <a class="btn-link fw-normal" href="/blogdetails?id=<%= blog.id %>">
                                            Đọc thêm <i class="icon fa fa-arrow-right"></i>
                                        </a>
                                    </div>
                                </div>
                                <!--== End Blog Item ==-->
                            </div>
                            <% }) %>
                    </div>

                </div>
            </div>
            <!--== End Blog Grid Area Wrapper ==-->

        </main>


        <!--== Start Footer Area Wrapper ==-->
        <footer class="footer-area">
            <%- include('../shares/footer') %>
        </footer>
        <!--== End Footer Area Wrapper ==-->


    </div>
    <!--== Wrapper End ==-->

    <!-- JS Vendor, Plugins & Activation Script Files -->

    <!-- Vendors JS -->
    <script src="/static/js/vendor/modernizr-3.11.7.min.js"></script>
    <script src="/static/js/vendor/jquery-3.6.0.min.js"></script>
    <script src="/static/js/vendor/jquery-migrate-3.3.2.min.js"></script>
    <script src="/static/js/vendor/bootstrap.bundle.min.js"></script>

    <!-- Plugins JS -->
    <script src="/static/js/plugins/swiper-bundle.min.js"></script>
    <script src="/static/js/plugins/fancybox.min.js"></script>
    <script src="/static/js/plugins/jquery.nice-select.min.js"></script>
    <script src="/static/js/plugins/jquery.countdown.min.js"></script>
    <script src="/static/js/plugins/isotope.pkgd.min.js"></script>

    <!-- Custom Main JS -->
    <script src="/static/js/main.js"></script>

</body>


